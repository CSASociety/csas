- title "Newsletters"
- if can? :create, Newsletter
  - edit_text "Upload Newsletter"
  - edit_path new_newsletter_path

%iframe{src: @current_newsletter.file.url, width: "750", height: "1000"}

%h2 Archive
%table
  %tr
    %th Date
    %th Download
  - @newsletters.each do |newsletter|
    %tr
      %td= link_to newsletter.created_at.strftime("%B, %Y"), newsletter
      %td= link_to newsletter.file_content_type, newsletter.file.url 
  
